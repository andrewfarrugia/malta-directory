    "preview": "astro preview",
    "check": "astro check",
    "seo:validate": "node ./scripts/validate-seo.mjs"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.0.0",
    "@astrojs/rss": "^4.0.0",
    "@astrojs/sitemap": "^3.0.0",
    "astro": "^5.0.0",
    "gray-matter": "^4.0.3",
    "zod": "^3.24.1"
  }
}
