{
  "name": "malta-service-hub",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "images:sync": "node ./scripts/sync-pexels-images.mjs",
    "images:sync:refresh": "node ./scripts/sync-pexels-images.mjs --refresh",
    "images:sync:all": "node ./scripts/sync-pexels-images.mjs --all",
    "images:validate": "node ./scripts/validate-pexels-quality.mjs",
